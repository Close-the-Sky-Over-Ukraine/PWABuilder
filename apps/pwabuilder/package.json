{
  "name": "pwabuilder",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "preinstall": "node ../../scripts/setupDeps.js",
    "test": "npm install -D playwright && npx playwright install-deps chromium && npx playwright test",
    "mocha": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha",
    "coverage": "nyc npm run mocha",
    "clean": "rimraf build/ && rimraf dist/",
    "build": "rimraf dist/ && cross-env NODE_ENV=production && rollup --config rollup.config.js",
    "start": "npm run build && npx nodemon server.js",
    "dev": "cross-env NODE_ENV=development && concurrently \"rollup --config rollup.config.dev.js --watch\" \"web-dev-server\"",
    "debug": "concurrently \"rollup --config rollup.config.dev.js --watch\" \"web-dev-server --config web-dev-server.debugging.config.mjs\"",
    "test2": "echo should run second"
  },
  "author": "",
  "license": "ISC",
